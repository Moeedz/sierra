      ],
      "name": "t:sections.image_with_text.presets.image_with_text.name",
      "settings": {
        "color_scheme": "scheme-83c8f934-b06b-498f-9443-09bcaf9992d7",
        "separate_section_with_border": false,
        "image": "shopify://shop_images/DSC0833-2_cceacde0-5870-4099-8803-ad6e04e79e3b.jpg",
